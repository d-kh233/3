import matplotlib.pyplot as plt
math=[2, 3, 3, 2, 5, 5, 3, 4, 5, 4]
plt.hist(math, bins = [2,3,4,5,6], color='pink')
plt.ylabel=("Кол-во учеников")
plt.xlabel=("Оценки")
plt.title= ("распределение оценок по математике среди учащихся")
plt.show()

